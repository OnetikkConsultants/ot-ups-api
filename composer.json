{
  "name": "onetikk/ot-ups-api",
  "type": "library",
  "description": "PHP UPS API",
  "keywords": [
    "ups",
    "api",
    "shipping"      
  ],  
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "dileep",
      "email": "dileep@onetikk.com"
    }    
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^9.3"
  },
  "suggest": {
    "ext-soap": "Required if you need the Tradeability API of UPS"
  },
  "autoload": {
    "psr-4": {
      "Onetikk\\ot-ups-api\\": "src"
    }
  },
  "extra": {  
    "laravel": {
      "providers": [
        "Onetikk\\ot-ups-api\\UpsApiServiceProvider"
      ]
    }
  }  
}