{
  "name": "onetikk/ot-ups-api",
  "type": "library",
  "description": "PHP UPS API",
  "keywords": [
    "ups",
    "api",
    "shipping"      
  ],
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "dileep",
      "email": "dileep@onetikk.com"
    }    
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^9.3"
  },
  "suggest": {
    "ext-soap": "Required if you need the Tradeability API of UPS"
  },
  "extra": {    
  },
  "autoload": {
    "psr-4": {
      "Ups\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ups\\Tests\\": [
        "tests/Ups/Tests",
        "tests/Ups/Tests/_includes"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}